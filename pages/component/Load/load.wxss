/* pages/loading/loading.wxss */
.loading{
    border-radius: 50%;
   
    z-index: 999!important;
    text-align: center;
    color: #f11f1f;
    position: fixed;
    top: -100rpx;
    width: 30%;
    left: 35%;
    
 
}
.loading view{
    padding: 5rpx;
    border-radius: 50%;
      border: 1 px solid rgba(0, 0, 0, 0.219);
    background: #fff;
     
}
.iw_btn{
    text-align: center;
    padding: 10rpx;
    left: 35%;
    position: fixed;
    bottom: 10rpx;
    width: 30%;
    border-radius: 20px;
    box-shadow: 2px 2px 2px 2px rgba(0,0,0,0.1);
    background: #f11f1f;
    color: #fff;
}